scalar DateTime

scalar File

type Mutation {
  signIn(credentials: SignInInput!): SignInPayload
  upload(file: File!): String
}

type Query {
  me: User
  track(id: ID!): Track
  tracks: [Track!]!
}

input SignInInput {
  email: String!
  password: String!
}

type SignInPayload {
  user: User!
}

type Track {
  artist: String!
  createdAt: DateTime!
  description: String
  id: ID!
  source: String!
  title: String!
  updatedAt: DateTime!
}

type User {
  email: String!
  id: ID!
  name: String!
}
